    "lint-staged": "15.2.10",
    "node-plop": "0.32.0",
    "plop": "4.0.1",
    "prettier": "^2.8.3",
    "remark": "15.0.1",
    "remark-cli": "12.0.1",
    "rimraf": "6.0.1",
    "syncpack": "13.0.0"
